{
  "$schema": "https://vega.github.io/schema/vega-lite/v4.json",
  "width": 800,
  "height": 500,
  "title": "House prices across Victoria",
  "projection": {"type": "equalEarth"},
  "layer": [
    {
      "data": {
        "url": "https://raw.githubusercontent.com/JaredNgu/Week9HW/main/VIC_LOCALITY_POLYGON_SHP.json",
        "format": {"type": "topojson", "feature": "VIC_LOCALITY_POLYGON_SHP"}
      },
      "mark": {"type": "geoshape", "fill": "lightgray", "stroke": "white"}
    },
    {
      "data": {
        "url": "https://raw.githubusercontent.com/JaredNgu/Week9HW/main/Melbourne_housing_FULL.csv"
      },
      "mark": {"type": "circle", "tooltip": {"content": "data"}},
      "encoding": {
        "longitude": {"field": "Longtitude", "type": "quantitative"},
        "latitude": {"field": "Lattitude", "type": "quantitative"},
        "size": {
          "field": "mag",
          "type": "quantitative",
          "scale": {"domain": [4, 8]}
        },
        "color": {
          "field": "depth",
          "type": "quantitative",
          "scale": {"scheme": "reds"}
        }
      }
    }
  ],
  "config": {}
}